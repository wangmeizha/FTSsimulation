        <Fill name="SFPB" comment="Some SSD Shell dimensions"  >
            <var name="Hhight" value="(0.02/tan(54*pi/180)+0.02/(2*tan(pi/5))+0.2)*sin(pi/5)" comment=" haut 1 "  />
            <var name="Khight" value="0.02/sin(54*pi/180)" comment=" haut 2 "  />
